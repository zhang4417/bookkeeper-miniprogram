/* components/notes/notes.wxss */
.wrapper{
  margin:8rpx 16rpx;
}
.note{
  display:flex;
  border:1px solid #eee;
  align-items: center;
  border-radius:8rpx;
}
.note-text{
  padding:3rpx 8rpx;
  color:#ccc;
  line-height: 100%;
}
.note-line{
  border-right:1px solid #ddd;
  height:16px;
  margin-right:8rpx;
}
.note-input{
  height:72rpx;
}
